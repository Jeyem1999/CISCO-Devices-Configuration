----------------------------> DAY 1 OF CCNA <-----------------------------------
CONFIGURATION OF CISCO DEVICES!! SWITCHES, IP CAMERA, IP TELEPHONE, CALL MANAGER

--------------------------------------------------------------------------------
##SHOW COMMANDS IN CISCO
SIIB- sh ip int brief
SVB- sh vlan brief
SCN- sh cdp neighbor
SMAT- sh mac address-table
SIDB- sh int dhcp binding
--------------------------------------------------------------------------------

##configure a core switch switch##

conf t
	hostname (give the a switch a name)
	enable secret pass
	service password-encryption
	no logging console
	no ip domain-lookup
	line console 0
		password pass
		login
		exec-timeout 0 0
		line vty 0 14
			password pass
			login
			exec-timeout 0 0
		int vlan 1 (best practice to do vlan 1 as mgmt)
		no shutdown (no shutdown means "turn on"
			ip add (give IP to mgmt and subnet)
			description (give vlan a name; in this case, MGMT) MGMT
		int vlan 10
		no shutdown
			ip add x.x.x.x x.x.x.x
			description _______
	(ADD VLANS AS REQUIRED)
end

##configure a leaf switch

conf t
	hostname (give leaf switch a name)
	enable secret pass
	service password-encryption
	no logging console
	no ip domain-lookup
	line console 0
		password pass
		login
		exec-timeout 0 0
		line vty 0 14
			password pass
			login
			exec-timeout 0 0
		int Gi 0/1
			no shutdown
				no switchport
				ip add (assign IP to gi 0/1 port and subnet for internet *FOC*)
		int vlan 1
			no shutdown
				ip add (IP of MGMT)
				description (name of vlan in this case, MGMT)
		int vlan 10
			no shutdown
				ip add x.x.x.x x.x.x.x
	(ADD VLANS AS REQUIRED)
end

##configure DHCP for vlans(LS)

conf t
	ip dhcp exlude-add x.x.x.x x.x.x.x (range of the IP you want to exlude from the dhcp of the vlan)
(ADD AS MANY DHCP EXCLLUDE-ADD AS THE NUMBER OF THE VLAN)
ip dhcp pool MGMT
	network x.x.x.0 x.x.x.x(subnet)
	default-router x.x.x.x (magament port for vlan 1 MGMT)
	domain-name MGMT.com
	dns-server x.x.x.x (add dns ip for the vlan)
(ADD VLANS AS REQUIRED)
end

##Create and Assign port for each VLANS

conf t
	vlan 10
		name _______
(ADD VLANS AS REQUIRED)
int fa 0/2
	switchport mode access
	switchport access vlan 10
int fa 0/3
	switchport mode access
	switchport voice vlan _______(VLAN# for VoIP)
(ADD PORT AS VLANS REQUIRED)
[For VoIP use voice insted of access]
int fa 0/5
	switchport mode access
	switchport voice vlan _______
	mls qos trust device cisco-phone
	switchport access vlan 1
(ADD PORT AS VOIP REQUIRED)
end

##Configure IP Cameras (MAC address required for IP Camera)
##Command for MAC Address --->sh mac address-table

conf t
	ip routing
	ip dhcp pool CAMERA1 (camera name)
	host x.x.x.x x.x.x.x (ip is where that VLAN of the camera resides)
	client-identifier xxxxxxxxxx(MAC address)
(ADD AS MANY CAMERA AS REQUIRED)

end

##CISCO UNIFIED CALL MANAGER EXPRESS Command

conf t
	hostname (give name for CUCM)
	enable secret pass
	service password-encryption
	no logging console
	no ip domain-lookup
	line console 0
		password pass
		login
		exec-timeout 0 0
		line vty 0 14
			password pass
			login
			exec-timeout 0 0
		int fa 0/0
			no shutdown
				ip add x.x.x.x x.x.x.x(give ip to the cucm and subnet)
end

##CUCM Analog Phones Configure

conf t
	dial-peer voice 1 pots
		destination-pattern 2200
			port 0/0/0
	dial-peer voice 2 pots
		destination-pattern 2201
			port 0/0/1
	dial-peer voice 3 pots
		destination-pattern 2002
			port 0/0/2
	dial-peer voice 4 pots
		destination-pattern 2003
			port 0/0/3
end

##IP Telephone

conf t
no telephony-service
telephony-service
	no auto assign
	no auto-reg-ephone
	max-ephones x (depending on how many ephones)
	max-dn 20
	ip source-address x.x.x.x(cucm address-source of dhcp ip) port 2000
	create cnf-files
ephone-dn1
	number 2211
ephone-dn2
	number 2222
(ADD AS MANY DN FOR EACH IP PHONE)
Ephone 1
	mac-address x.x.x.x.x.x (enter mac address of IP phone)
	type 8945
	button 1:8 2:7 3:6 4:5
	restart
Ephone 2
	mac-address x.x.x.x.x.x (enter mac address of IP phone)
	type 8945
	button 1:4 2:3 3:2 4:1
	restart
end

##Enable Video calls

conf t
ephone 1
	video
	voice service voip
	h323
	call start slow
ephone 2
	video
	voice service voip
	h323
	call start slow
end

##Allow incoming calls

conf t
	voice service voip
	ip address trusted list
	ipv4 0.0.0.0 0.0.0.0
exit

##Allow outgoing call from CUCM to other CUCM

conf t
	dial-peer voice x voip (x = monitor number of other users)
	destination-pattern x
	session target ipv4 _.x._._ (ip of the CUCM while x is still the monitor number of other users)
	codec g711ULAW
(ADD AS MY AS VOIP REQUIRES)
end

##EDGE ROUTE CONFIGURATION

conf t
	hostname edge
	enable secret pass
	service password-encryption
	no logging console
	no ip domain-lookup
	line console 0
		password pass
		login
		exec-timeout 0 0
		line vty 0 14
			password pass
			login
			exec-timeout 0 0
		int gi 0/0/0
			description FiberOptic-to-switch
			ip add x.x.x.x x.x.x.x (ip for edge)
			no shutdown
		int gi 0/0/1
			description PLDT-ME-WAN
			ip add 200.0.0.22 255.255.255.0
			no shutdown
		int  loopback 0
		description VIRTUALIP-FOR-ROUTING
			ip add 22.0.0.1 255.255.255.255
end

##OPEN SHORTEST PATH FIRST/OSPF CONFIGURATION
##EDGE

config t
	router ospf 1
	router-id 22.0.0.1
	network 200.0.0.0  0.0.0.255 area 0
	network 10.22.22.0  0.0.0.255 area 0
	network 22.0.0.1  0.0.0.0 area 0
	Interface gi 0/0/0
	ip ospf network point-to-point
end

##LeafSW

config t
	router ospf 1
	router-id 10.22.22.4
	network 10.22.0.0  0.0.255.255 area 0
	int gi 0/1
	ip ospf network point-to-point
end

##CUCM

config t
	router ospf 1
	router-id 10.22.100.8
	network 10.22.100.0  0.0.0.255  area 0
end


